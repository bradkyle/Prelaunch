  },
  "homepage": "https://github.com/bradkyle/Prelaunch#readme",
  "dependencies": {
    "aws-sdk": "^2.597.0",
    "aws-serverless-express": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dynamodb": "^1.2.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "faker": "^4.1.0",
    "getmac": "^5.0.0",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "public-ip": "^4.0.0",
