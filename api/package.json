{
  "name": "prelaunch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "deploy": "npm run clean && npm run build && aws lambda update-function-code --function-name employees --zip-file fileb://build.zip --publish",
    "clean": "rm build.zip",
    "build": "zip -r build.zip node_modules index.js app.js routes.js",
    "create-local-database": "aws dynamodb create-table --cli-input-json file://axiom-prelaunch-table-model.json --endpoint-url http://localhost:8080",
    "dynamodb-local-run": "docker-compose up",
    "start": "TABLE='ViralPrelaunch' node app-local"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradkyle/Prelaunch.git"
  },
